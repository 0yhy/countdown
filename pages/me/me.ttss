.wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.top {
  width: 100%;
  height: 300rpx;
}
.row {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
  width: 90%;
  height: 100rpx;
  margin: 0;
  padding: 0;
  background: transparent;
}
.row::after {
  border: none;
}
.row-img {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.row-text {
  font-size: 28rpx;
}
